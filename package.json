{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio with React using Typescript.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "homepage": "https://roottool.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/roottool/portfolio/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf .next",
    "dev": "next dev",
    "build": "npm-run-all clean --parallel build:*",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint . --color --ext .js,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "run-p -c fix:*",
    "fix:eslint": "eslint . --fix --color --ext .js,.ts,.tsx",
    "format": "prettier --write .",
    "test": "run-s test:*",
    "test:jest": "echo \"Write tests!\"",
    "test:type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.js|tsx?": [
      "prettier --write",
      "eslint --fix --color"
    ],
    "*.json|md": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.5.1",
    "@radix-ui/react-toolbar": "^0.1.5",
    "@stitches/react": "^1.2.7",
    "axios": "0.27.2",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "swr": "^1.2.1"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.4.2",
    "@next/eslint-plugin-next": "^12.0.8",
    "@types/node": "^17.0.24",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "redux-devtools-extension": "^2.13.9",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.2"
  }
}
