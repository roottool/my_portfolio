{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio with React and Typescript.",
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "homepage": "https://roottool.netlify.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/roottool/portfolio/issues"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "prepare": "npx is-ci || husky install",
    "start": "react-scripts start",
    "build": "run-p build:*",
    "build:react": "react-scripts build",
    "lint": "run-p -c lint:*",
    "lint:check-types": "tsc --noEmit",
    "lint:eslint": "eslint . --color --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "run-p -c fix:*",
    "fix:eslint": "eslint . --fix --color --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write .",
    "test": "run-p test:*",
    "test:check-types": "tsc --noEmit",
    "test:jest": "echo \"Write tests!\""
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "importSort": {
    ".js, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "*.js|ts|tsx": [
      "yarn fix",
      "yarn format"
    ],
    "*.json|md": [
      "yarn format"
    ],
    ".eslint|prettierrc": [
      "yarn format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.5.1",
    "axios": "^0.23.0",
    "connected-react-router": "^6.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.4",
    "@types/react-redux": "^7.1.19",
    "@types/react-router-dom": "^5.3.1",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "husky": "^7.0.2",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "redux-devtools-extension": "^2.13.9",
    "typescript": "^4.4.4"
  }
}
