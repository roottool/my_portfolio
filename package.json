{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio with React using Typescript.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "homepage": "https://roottool.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/roottool/portfolio/issues"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rimraf .next",
    "dev": "next dev",
    "build": "npm-run-all clean --parallel build:*",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-s -c lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint . --color --ext .js,.ts,.tsx",
    "lint:tsc": "tsc --noEmit",
    "fix": "run-s -c fix:*",
    "fix:prettier": "prettier --write .",
    "fix:eslint": "eslint . --fix --color --ext .js,.ts,.tsx",
    "test": "run-s test:*",
    "test:jest": "echo \"Write tests!\""
  },
  "lint-staged": {
    "*.js|tsx?": [
      "prettier --write",
      "eslint --fix --color"
    ],
    "*.md|json|ya?ml": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@radix-ui/react-toolbar": "^0.1.5",
    "@stitches/react": "1.2.8",
    "axios": "0.27.2",
    "next": "12.1.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "4.4.0",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.1.6",
    "@types/node": "17.0.30",
    "@types/react": "17.0.47",
    "@types/react-dom": "17.0.17",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "13.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  }
}
