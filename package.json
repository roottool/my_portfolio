{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My portfolio with React using Typescript.",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "roottool",
    "url": "https://github.com/roottool"
  },
  "homepage": "https://roottool.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/roottool/portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/roottool/portfolio/issues"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "clean": "rimraf .next .swc",
    "dev": "next dev",
    "build": "npm-run-all clean --parallel build:*",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-s -c lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint . --color --ext .js,.ts,.tsx",
    "fix": "run-s -c fix:*",
    "fix:prettier": "prettier --cache --write .",
    "fix:eslint": "eslint . --fix --color --cache --ext .js,.ts,.tsx",
    "test": "run-s -c test:tsc \"test:jest {@}\"",
    "test:tsc": "tsc --noEmit",
    "test:jest": "jest --colors"
  },
  "lint-staged": {
    "*.{js,ts?(x)}": [
      "prettier --write",
      "eslint --fix --color"
    ],
    "*.{md,json,y?(a)ml}": [
      "prettier --write"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "dependencies": {
    "@material-ui/core": "4.12.4",
    "@material-ui/icons": "4.11.3",
    "@radix-ui/react-toolbar": "1.0.2",
    "@stitches/react": "1.2.8",
    "got": "12.5.3",
    "ky": "0.33.2",
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-icons": "4.7.1",
    "swr": "2.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.1.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jest": "29.2.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.3",
    "is-ci": "3.0.1",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "lint-staged": "13.1.0",
    "msw": "0.49.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.3",
    "rimraf": "4.1.1",
    "typescript": "4.9.4"
  }
}
